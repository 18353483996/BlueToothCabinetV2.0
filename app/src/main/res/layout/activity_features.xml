<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.xiaoyuzhou.bluetoothcabinetv10.FeaturesActivity">

    <TextView
        android:id="@+id/tv_features_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="功能选择页面"
        android:textSize="30sp"
        android:textColor="#000000"
        android:layout_gravity="center_horizontal"
        />

    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/tv_OpenCabinet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textColor="#f10000"
        android:text="智能货柜管理"/>
    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/tv_OvernightApplication"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="实验室留宿申请"
        android:textColor="#f10000"
        android:textSize="20sp"/>



</LinearLayout>
